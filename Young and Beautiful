#Young and Beautiful by Lana Del Ray

use_bpm 114
use_synth :sine
humm = "C:/Users/Isabel Oropesa/Downloads/Young and Beautiful sonic pi recording.wav"
notes = [:e4, :d4, :e4, :d4, :d4, :e4, :d4, :e4, :d4, :d4, :e4, :d4, :b3, :e4, :e4, :e4, :e4, :d4]
sleep = [1, 1, 0.5, 0.35, 1, 1, 1, 0.5, 0.35, 1, 0.5, 0.5, 0.5, 0.50, 0.5, 0.5, 0.5, 1]
x = 0
y = 0

live_loop :song1 do
  sample humm
  18.times do
    play notes[x]
    sleep sleep[y]
    x = x + 1
    y = y + 1
  end
  x = 0
  y = 0
end
